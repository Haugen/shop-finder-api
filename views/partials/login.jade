div.row
	div.page-header
		h3 Login

div.row
	div.col-md-4.col-md-offset-4
		div.jumbotron
			form.form-horizontal
				div.form-group
					input.form-control(type="email", placeholder="Email", ng-model="loginForm.email")
				div.form-group
					input.form-control(type="password", placeholder="Password", ng-model="loginForm.password")
				div.form-group
					label.col-sm-offset-2.col-sm-10
					button.btn.btn-primary(type="submit") Login
